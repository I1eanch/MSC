{
  "name": "admin-web",
  "version": "1.0.0",
  "description": "Next.js admin web application",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write pages/**/*.tsx components/**/*.tsx lib/**/*.ts styles/**/*.css",
    "format:check": "prettier --check pages/**/*.tsx components/**/*.tsx lib/**/*.ts styles/**/*.css",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next dist"
  },
  "dependencies": {
    "@packages/api-client": "workspace:*",
    "@packages/analytics-sdk": "workspace:*",
    "@packages/ui-kit": "workspace:*",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "typescript": "^5.0.0"
  }
}
